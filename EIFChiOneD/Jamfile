########### EIFChiOneD lib #############

project :
	requirements <include>./
         <include>$(TOP)/Core
	<cxxflags>-ftemplate-depth-256
	<cxxflags>-std=c++0x
	;

lib EIFChiOneD : 
        [ glob *.cpp ]
        $(TOP)/Core//ComPWA 
         ;

install dist : EIFChiOneD : <location>$(TOP)/lib ;

